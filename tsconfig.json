{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "./compiled"
  },
  "files": [
    "typings/index.d.ts",
    "./app/main.ts",
    "./module/module.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
