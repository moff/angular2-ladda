{
  "name": "angular2-ladda",
  "version": "1.0.7",
  "description": "Angular 2 module, implements Ladda",
  "files": [
    "module/*.d.ts",
    "module/*.js",
    "module/*.js.map",
    "module/*.json"
  ],
  "main": "./module/module.js",
  "types": "./module/module.d.ts",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "prepublish": "ngc -p ./tsconfig.publish.json"
  },
  "keywords": [
    "angular2",
    "angular",
    "npm",
    "package",
    "module",
    "ladda",
    "angular2-ladda"
  ],
  "author": "Paul Moff",
  "repository": {
    "type": "git",
    "url": "https://github.com/moff/angular2-ladda.git"
  },
  "license": "MIT",
  "dependencies": {
    "ladda": "^1.0.0"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0"
  },
  "devDependencies": {
    "@angular/common": "~2.4.4",
    "@angular/compiler": "~2.4.4",
    "@angular/compiler-cli": "~2.4.4",
    "@angular/core": "~2.4.4",
    "@angular/forms": "~2.4.4",
    "@angular/http": "~2.4.4",
    "@angular/platform-browser": "~2.4.4",
    "@angular/platform-browser-dynamic": "~2.4.4",
    "@angular/platform-server": "~2.4.4",
    "@angular/router": "~3.4.4",
    "@angular/upgrade": "~2.4.4",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.41",
    "@types/ladda": "^0.9.28",
    "@types/node": "^7.0.0",
    "angular-in-memory-web-api": "~0.2.4",
    "concurrently": "^3.1.0",
    "core-js": "^2.4.1",
    "lite-server": "^2.2.2",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.3",
    "systemjs": "0.19.42",
    "typescript": "^2.1.5",
    "zone.js": "^0.7.6"
  }
}
